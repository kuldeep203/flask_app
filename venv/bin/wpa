#!/home/kuldeep/flask-app/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'wpa-supplicant==0.2','console_scripts','wpa'
__requires__ = 'wpa-supplicant==0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('wpa-supplicant==0.2', 'console_scripts', 'wpa')()
    )
